{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "examplePhrases": [
            "Alexa open Appointment Scheduler",
            "Alexa start Appointment Scheduler",
            "Alexa ask Appointment Scheduler to schedule an appointment for tomorrow"
          ],
          "name": "Appointment Scheduler",
          "smallIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-sm.png",
          "largeIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-lg.png"
        }
      }
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda"
        }
      }
    },
    "permissions": [
      {
        "name": "alexa::profile:name:read"
      },
      {
        "name": "alexa::profile:email:read"
      },
      {
        "name": "alexa::profile:mobile_number:read"
      }
    ],
    "manifestVersion": "1.0"
  }
}